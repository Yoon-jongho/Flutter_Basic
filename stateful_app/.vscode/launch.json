  "version": "0.2.0",
  "configurations": [
    {
      "name": "stateful_app",
      "request": "launch",
      "type": "dart",
      "program": "lib/main.dart",
      "args": ["--no-sound-null-safety"],
      "flutterMode": "debug",
      "flutterPlatform": "android",
      "env": {
        "FLUTTER_LOG_FILTER": "I/MESA"
      }
    },
    {
      "name": "stateful_app (profile mode)",
      "request": "launch",
      "type": "dart",
      "flutterMode": "profile"
    },
    {
      "name": "stateful_app (release mode)",
      "request": "launch",
      "type": "dart",
      "flutterMode": "release"
    }
  ]
}
